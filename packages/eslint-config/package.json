{
  "name": "@nuxt/eslint-config",
  "version": "0.6.2",
  "description": "ESLint config for Nuxt projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/eslint.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "main": "./dist/legacy.cjs",
  "module": "./dist/legacy.mjs",
  "types": "./dist/legacy.d.ts",
  "exports": {
    ".": {
      "import": "./dist/legacy.mjs",
      "require": "./dist/legacy.cjs"
    },
    "./flat": {
      "import": "./dist/flat.mjs",
      "require": "./dist/flat.cjs"
    },
    "./legacy": {
      "import": "./dist/legacy.mjs",
      "require": "./dist/legacy.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "flat": [
        "./dist/flat.d.ts"
      ],
      "legacy": [
        "./dist/legacy.d.ts"
      ],
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0",
    "eslint-plugin-format": "*"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-format": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.4.1",
    "@clack/prompts": "^0.8.1",
    "@eslint/js": "catalog:",
    "@nuxt/eslint-plugin": "workspace:*",
    "@stylistic/eslint-plugin": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint-config-flat-gitignore": "catalog:",
    "eslint-flat-config-utils": "catalog:",
    "eslint-plugin-import-x": "catalog:",
    "eslint-plugin-jsdoc": "catalog:",
    "eslint-plugin-regexp": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "eslint-plugin-vue": "catalog:",
    "eslint-processor-vue-blocks": "^0.1.2",
    "eslint-merge-processors": "^0.1.0",
    "globals": "catalog:",
    "local-pkg": "catalog:",
    "pathe": "catalog:",
    "vue-eslint-parser": "catalog:"
  },
  "devDependencies": {
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
