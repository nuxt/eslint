{
  "name": "@nuxt/eslint-config",
  "version": "0.3.0-beta.2",
  "description": "ESLint config for Nuxt projects",
  "repository": { 
    "type": "git", 
    "url": "git+https://github.com/nuxt/eslint.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/flat.d.ts",
      "import": "./dist/flat.mjs",
      "require": "./dist/flat.cjs"
    },
    "./legacy": {
      "types": "./dist/legacy.d.ts",
      "import": "./dist/legacy.mjs",
      "require": "./dist/legacy.cjs"
    }
  },
  "main": "./dist/flat.cjs",
  "module": "./dist/flat.mjs",
  "types": "./dist/flat.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "@eslint/js": "^8.57.0",
    "@nuxt/eslint-plugin": "workspace:*",
    "@rushstack/eslint-patch": "^1.8.0",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint-plugin-vue": "^9.23.0",
    "pathe": "^1.1.2",
    "vue-eslint-parser": "^9.4.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.6",
    "eslint": "^8.57.0",
    "typescript": "^5.4.2"
  }
}
